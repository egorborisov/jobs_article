            "avatar_hash": "ga906b4a7894",
            "image_72": "https:\/\/secure.gravatar.com\/avatar\/02a906b4a789431dc4b65ddd633c616f.jpg?s=72&d=https%3A%2F%2Fa.slack-edge.com%2Fdf10d%2Fimg%2Favatars%2Fava_0021-72.png",
            "first_name": "",
